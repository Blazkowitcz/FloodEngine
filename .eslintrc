{
    "root": true,
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "parser": "babel-eslint"
    },
    "rules": {
        "eqeqeq": "error",
        "curly": "error",
        "semi": "error",
        "no-console": "error",
        "require-jsdoc": "error"
    },
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ]
}